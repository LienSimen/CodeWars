using System.Text.RegularExpressions;
â€‹
public class DnaStrand 
    {
        public static string MakeComplement(string dna)
        {
            return Regex.Replace(dna, "[ATCG]", match => match.Value switch
            {
                "A" => "T",
                "T" => "A",
                "C" => "G",
                "G" => "C",
                _ => match.Value
            });
        }
    }