using System;
using System.Diagnostics.CodeAnalysis;
public class Predicter
{
      public static int PredictAge(int age1, int age2, int age3, int age4, int age5, int age6, int age7, int age8)
      {
        int newAge1 = age1 * age1;
        int newAge2 = age2 * age2;
        int newAge3 = age3 * age3;
        int newAge4 = age4 * age4;
        int newAge5 = age5 * age5;
        int newAge6 = age6 * age6;
        int newAge7 = age7 * age7;
        int newAge8 = age8 * age8;
        int sum = newAge1 + newAge2 + newAge3 + newAge4 + newAge5 + newAge6 + newAge7 + newAge8;
        int sqrt = (int)Math.Sqrt(sum);
        int sqrtDividedByTwo = sqrt / 2;
        return sqrtDividedByTwo;
      }
}