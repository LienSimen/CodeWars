using System;
using System.Collections.Generic;
using System.Linq;
â€‹
public class Remover
{
  public static List<int> RemoveSmallest(List<int> numbers)
  {
    if (numbers == null || numbers.Count == 0)
    {
        return numbers;
    }
    numbers.Remove(numbers.Min());
    return numbers;
  }
}