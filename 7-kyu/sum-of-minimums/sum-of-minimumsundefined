using System;
â€‹
class Kata
{
  public static int SumOfMinimums(int[,] numbers)
  {
    int sum = 0;
    for (int i = 0; i < numbers.GetLength(0); i++)
    {
        int min = int.MaxValue;
        for (int j = 0; j < numbers.GetLength(1); j++)
        {
            if (numbers[i, j] < min)
            {
                min = numbers[i, j];
            }
        }
        sum += min;
    }
    return sum;
  }
}