 
using System.Collections.Generic;
​
public class Kata()
    {
        public static string HowManyDalmatians(int n)
        {
            List<string> dogs = new List<string>()
            {
                "Hardly any",
                "More than a handful!",
                "Woah that's a lot of dogs!",
                "101 DALMATIONS!!!"
            };
            var number = n;
            string respond;
            if (number <= 10) 
            {
                respond = dogs[0];
            }
            else if (number <= 50)
            {
                respond = dogs[1];
            }
            else if (number == 101)
            {
                respond = dogs[3];
            }
            else 
            {
                respond = dogs[2];
            }
            return respond;
        }
    }