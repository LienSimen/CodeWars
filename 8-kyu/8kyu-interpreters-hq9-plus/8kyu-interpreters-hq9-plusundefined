 
using System.Text;
​
public class HQ9
{
  public static string Interpret(string code)
  {
    if (code.Contains('H'))
    {
      return "Hello World!";
    }
    if (code.Contains('Q'))
    {
      return code;
    }
    if(code == "9")
    {
      var numberOfBottles = 99;
      var lyrics = new StringBuilder();
      for (int i = numberOfBottles; i > 0; i--)
      {
        lyrics.AppendLine($"{i} bottles of beer on the wall, {i} bottles of beer.");
        lyrics.AppendLine($"Take one down and pass it around, {i - 1} bottles of beer on the wall.");
        lyrics.AppendLine();
      }
      return lyrics.ToString();
    }return null;
  }
}