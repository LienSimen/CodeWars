 
using System.Text;
using System.Linq;
​
public class HQ9
{
  public static string Interpret(string code)
  {
    if (code.Contains('H'))
    {
      return "Hello World!";
    }
    if (code.Contains('Q'))
    {
      return code;
    }
    if(code.Contains('9'))
    {
      var numberOfBottles = 99;
      for (int i = numberOfBottles; i > 0; i--)
      {
        return $"{i} bottles of beer on the wall, {i} bottles of beer.\nTake one down and pass it around, {i - 1} bottles of beer on the wall.";
      }
​
    }return null;
  }
}