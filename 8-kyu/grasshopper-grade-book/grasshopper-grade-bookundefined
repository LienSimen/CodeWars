using System;
â€‹
public class Kata
{
public static char GetGrade(int s1, int s2, int s3)
{
    var score = (s1 + s2 + s3) / 3;
â€‹
    if (score >= 90)
        return 'A';
    else if (score >= 80)
        return 'B';
    else if (score >= 70)
        return 'C';
    else if (score >= 60)
        return 'D';
    else
        return 'F';
}
}